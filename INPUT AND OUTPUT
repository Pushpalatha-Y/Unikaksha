#Hello World
print('Hello World')

#Hello World with tab
print('Hello World \t Hello World')

#Hello World with new line
print('Hello World \n Hello World')

#integer input
a=int(input())
print(a)

#float input
a=float(input())
print('%.2f'%a)

#character input
a=(input())
print(a)

#string input
a=str(input())
print(a)

#Ascii-1
a=input()
print(orda(a))

#Ascii-2
a=int(input())
print(chr(a))
